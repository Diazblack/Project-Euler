

var prime = function() 
{
	var pvalue =  document.getElementById.("pvalue").value ;
	var prime = [];
	var factor = [];
	var k= 0;
	var l = 0;

		for (i=2 ; i <= pvalue ; i++ ) 
		{
			for (j=2 ; j<=i ; j++)   
			{
				if (j % i === 0)
				{
					if(j === i ) 
					{
						i = prime[k];
						k++;
					}
				}
			}
	for(v=0 ; v<=k ; v++)
	{
		if(pvalue % prime [v] === 0){
			factor[l]=prime[v];
			l++;
		}
	}
}
